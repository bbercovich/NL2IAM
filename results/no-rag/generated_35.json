{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::ACCOUNT_ID:user/myuser"
      },
      "Action": "s3:*",
      "Resource": "arn:aws:s3:::prod--testfiles/*"
    },
    {
      "Effect": "Deny",
      "Principal": {
        "AWS": "*"
      },
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::prod--testfiles/*",
      "Condition": {
        "StringNotEquals": {
          "aws:username": [
            "root",
            "myuser"
          ]
        }
      }
    }
  ]
}