{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::account-id:user/myuser"
      },
      "Action": "s3:*",
      "Resource": "arn:aws:s3:::prod--testfiles/*"
    },
    {
      "Effect": "Deny",
      "NotPrincipal": {
        "AWS": [
          "arn:aws:iam::account-id:root",
          "arn:aws:iam::account-id:user/myuser"
        ]
      },
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::prod--testfiles/*"
    }
  ]
}