{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::xxx/*",
      "Condition": {
        "IpAddress": {
          "aws:SourceIp": [
            "0.0.0.0"
          ]
        }
      }
    },
    {
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::xxx/*",
      "Condition": {
        "StringNotLike": {
          "aws:Referer": [
            "test.com/*",
            "http://test.com/*"
          ]
        }
      }
    }
  ]
}